@model IEnumerable<OdeToFoodFour.Models.RestaurantReview>

@{
    ViewBag.Title = "Index";
}

<h2>@ViewBag.Title</h2>
<p>We are now showing the latest @Model.Count() reviews</p>

@foreach (var item in Model)
{
  @Html.Partial("_Review", item) 
}

<p>
    @Html.ActionLink("Create New", "Create")
</p>

